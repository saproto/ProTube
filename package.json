{
    "name": "backend",
    "version": "0.0.1",
    "description": "Backend part of protube",
    "main": "protube.js",
    "scripts": {
        "postinstall": "cd vue-remote && npm install",
        "build": "cd vue-remote && npm run build",
        "vue-run-dev": "cd vue-remote && npm run serve -- --port 8083",
        "start": "node protube.js",
        "watch": "nodemon protube.js",
        "dev": "run-p -r start vue-run-dev",
        "madge": "madge ./v2.js -i ./test.jpg",
        "dev-watch": "concurrently --kill-others -n VUE,NODEJS -c bgGreen.bold,bgBlue.bold 'cd ./vue-remote && npm run serve -- --port 8083' 'nodemon ./protube.js'"
    },
    "dependencies": {
        "colors": "^1.4.0",
        "connect-history-api-fallback": "^1.6.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-bearer-token": "^2.4.0",
        "express-mysql-session": "^2.1.8",
        "express-session": "^1.17.2",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "mysql2": "^2.3.3",
        "node-fetch": "^2.6.7",
        "npm-run-all": "^4.1.5",
        "passport": "^0.6.0",
        "passport-oauth2": "^1.6.1",
        "popyt": "^5.0.0",
        "socket.io": "^4.1.3",
        "socket.io-client": "^4.5.2",
        "winston": "^3.7.2",
        "winston-daily-rotate-file": "^4.6.1",
        "youtubei": "0.0.1-rc.32"
    },
    "devDependencies": {
        "autoprefixer": "^10.3.1",
        "concurrently": "^7.3.0",
        "electron": "^13.1.7",
        "madge": "^5.0.1",
        "nodemon": "^2.0.19"
    }
}
