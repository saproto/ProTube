{
  "name": "backend",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest ./tests",
    "build": "npm run routes:generate && tsc --build --verbose ./tsconfig.build.json",
    "dev": "npm run routes:generate && node --es-module-specifier-resolution=node --import ./bootstrap.js src/main.ts",
    "debug": "node --es-module-specifier-resolution=node --import ./bootstrap.js src/test.ts",
    "routes:generate": "node --es-module-specifier-resolution=node --import ./bootstrap.js src/generateRoutes.ts",
    "production": "node --conditions=production ./dist/main.js"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "imports": {
    "#App/*": {
      "production": "./dist/*",
      "default": "./src/*"
    },
    "#Routes/*": {
      "production": "./dist/Routes/*",
      "default": "./src/Routes/*"
    },
    "#Controllers/*": {
      "production": "./dist/Controllers/*",
      "default": "./src/Controllers/*"
    },
    "#Models/*": {
      "production": "./dist/Models/*",
      "default": "./src/Models/*"
    },
    "#Services/*": {
      "production": "./dist/Services/*",
      "default": "./src/Services/*"
    },
    "#Schemas/*": {
      "production": "./dist/Schemas/*",
      "default": "./src/Schemas/*"
    },
    "#Config.js": {
      "production": "./dist/Kernel/Services/Config.js",
      "default": "./src/Kernel/Services/Config.js"
    },
    "#Kernel/*": {
      "production": "./dist/Kernel/*",
      "default": "./src/Kernel/*"
    },
    "#SocketControllers/*": {
      "production": "./dist/SocketControllers/*",
      "default": "./src/SocketControllers/*"
    },
    "#Middlewares/*": {
      "production": "./dist/Middlewares/*",
      "default": "./src/Middlewares/*"
    },
    "#SocketMiddlewares/*": {
      "production": "./dist/SocketMiddlewares/*",
      "default": "./src/SocketMiddlewares/*"
    },
    "#Jobs/*": {
      "production": "./dist/Jobs/*",
      "default": "./src/Jobs/*"
    }
  },
  "description": "",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^5.2.0",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/oauth2": "^7.2.1",
    "@fastify/routes": "^5.1.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.2",
    "@mgcrea/fastify-session": "github:Menkveld-24/fastify-session#build",
    "@mgcrea/fastify-session-redis-store": "^1.0.0",
    "axios": "^1.4.0",
    "connect-redis": "^7.1.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.19.2",
    "fastify-plugin": "^4.5.1",
    "fastify-socket.io": "^4.0.0",
    "fastify-socketio-session": "github:Menkveld-24/fastify-socketio-session",
    "fastify-type-provider-zod": "^1.1.9",
    "fs-extra": "^11.1.1",
    "ioredis": "^5.3.2",
    "jest": "^29.6.1",
    "module-alias": "^2.2.3",
    "mysql2": "^3.5.0",
    "node-cron": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "socket.io": "^4.7.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.2.2",
    "zod-to-ts": "^1.2.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.4.1",
    "@types/node-cron": "^3.0.11",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "types-fastify-socket.io": "^0.0.1"
  }
}
